#include "common-mt7621/rutm.dtsi"
#include "common-mt7621/eth/1-lan-port.dtsi"
#include "common-mt7621/tpm.dtsi"

// PWR, RST |G0|G10|
// ETH      |G41-G43|G45|
// 4G       |G5|G7|G8|G12|G13|G17|G46-G48|
// MISC     |G6|G14|G16|G18|G44|
/ {
	model = "Teltonika RUTM20";

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		gpio_modem_reset {
			gpio-export,name = "modem_reset";
			gpio-export,output = <0>;
			gpios = <&gpio 13 GPIO_ACTIVE_HIGH>;
		};

		gpio_modem_power {
			gpio-export,name = "modem_power";
			gpio-export,output = <1>; // tmp
			gpios = <&gpio 17 GPIO_ACTIVE_HIGH>;
		};

		gpio_sim_select {
			gpio-export,name = "sim_sel";
			gpio-export,output = <0>;
			gpios = <&gpio 5 GPIO_ACTIVE_HIGH>;
		};

		gpio_tpm_reset {
			gpio-export,name = "tpm_reset";
			gpio-export,output = <1>;
			gpios = <&gpio 14 GPIO_ACTIVE_HIGH>;
		};
	};

	leds {
		led_gen_red {
			label = "mob_gen_red";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};

		led_gen_green {
			label = "mob_gen_green";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
		};

		led_gen_blue {
			label = "mob_gen_blue";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
		};

		led_ssid_red {
			label = "mob_ssid_red";
			gpios = <&gpio 46 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "timer"; // only
		};

		led_ssid_green {
			label = "mob_ssid_green";
			gpios = <&gpio 48 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "timer"; //only
		};

		led_ssid_blue {
			label = "mob_ssid_blue";
			gpios = <&gpio 47 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "timer"; // only
		};
	};

	tlt_gpios {
		gpiochip_info {
			hw_0 {
				gpiochip_count = <3>;
				/delete-property/ gpiochip_3;
			};
		};

		poeman {
			GPIO_16 {
				line_name = "GPIO_4PIN_SIGNAL";
				//active_low
			};
			GPIO_18 {
				line_name = "GPIO_T2P_SIGNAL";
			};
			GPIO_6 {
				line_name = "GPIO_PG_SIGNAL";
			};
			GPIO_44 {
				line_name = "GPIO_PSE_EN";
			};
		};

		misc {
			GPIO_13 {
				line_name = "GPIO_MODEM_RESET_1";
			};
			GPIO_17 {
				line_name = "GPIO_MODEM_POWER_1";
			};
		};
	};

	poeman {
		bus = "/dev/i2c-0";
		pse_en = "GPIO_PSE_EN";
		budget@0 {
			GPIO_PG_SIGNAL = <0>;
			budget = <0>;
		};
		budget@1 {
			GPIO_T2P_SIGNAL = <0>;
			GPIO_PG_SIGNAL = <1>;
			budget = <30200>;
			allowed_deficit = <600>;
		};
		budget@2 {
			GPIO_T2P_SIGNAL = <1>;
			GPIO_PG_SIGNAL = <1>;
			budget = <15000>;
			max_port_power = <7000>;
			allowed_deficit = <300>;
		};
	};
};

&mnfinfo {
	sim_cfg {
		default = "1111100_1112000_1121200";
		keep-sim-presence;
	};
};

&i2c {
	hwmon@0x48 {
		compatible = "ti,tla2021";
		reg = <0x48>;
	};
};

&gmac1 {
	phy-handle = <&ethphy0>;
};

&ethphy0 {
	/delete-property/ interrupts;
};
