#include "mt7621-teltonika-rutm-common.dtsi"

/ {
	compatible = "teltonika,rutm", "mediatek,mt7621-soc";
	model = "Teltonika RUTM20";

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 10 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		gpio_modem_reset {
			gpio-export,name = "modem_reset";
			gpio-export,output = <0>;
			gpios = <&gpio 13 GPIO_ACTIVE_HIGH>;
		};

		gpio_modem_power {
			gpio-export,name = "modem_power";
			gpio-export,output = <0>; // tmp
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		yellow_eth_port_1 {
			gpio-export,name = "yellow:eth_port_1";
			gpio-export,output = <0>;
			gpios = <&gpio 45 GPIO_ACTIVE_HIGH>;
		};

		yellow_eth_port_0 {
			gpio-export,name = "yellow:eth_port_0";
			gpio-export,output = <0>;
			gpios = <&gpio 42 GPIO_ACTIVE_HIGH>;
		};

		green_eth_port_1 {
			gpio-export,name = "green:eth_port_1";
			gpio-export,output = <0>;
			gpios = <&gpio 41 GPIO_ACTIVE_HIGH>;
		};

		green_eth_port_0 {
			gpio-export,name = "green:eth_port_0";
			gpio-export,output = <0>;
			gpios = <&gpio 43 GPIO_ACTIVE_HIGH>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_power {
			label = "power";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
			default-state = "on";
			retain-state-shutdown;
		};

		led_gen_red {
			label = "mob_gen_red";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};

		led_gen_green {
			label = "mob_gen_green";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
		};

		led_gen_blue {
			label = "mob_gen_blue";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
		};

		led_ssid_red {
			label = "mob_ssid_red";
			gpios = <&gpio 46 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "timer"; // only
		};

		led_ssid_green {
			label = "mob_ssid_green";
			gpios = <&gpio 48 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "timer"; //only
		};

		led_ssid_blue {
			label = "mob_ssid_blue";
			gpios = <&gpio 47 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "timer"; // only
		};
	};

	tlt_gpios { // Modify according to wiki page "DTS syntax with Libgpiod changes"
		gpiochip_info {
			hw_0 {
				hwver = <0>;
				gpiochip_count = <3>;

				gpiochip_0 = <32>; // Built-in
				gpiochip_1 = <32>; // Built-in
				gpiochip_2 = <32>; // Built-in
			};
		};

		ledman {
			GPIO_0 {
				line_name = "LED_POWER";
				active_low;
			};
			GPIO_43 {
				line_name = "LED_ETH_GREEN_1";
			};
			GPIO_41 {
				line_name = "LED_ETH_GREEN_2";
			};
			GPIO_42 {
				line_name = "LED_ETH_YELLOW_1";
			};
			GPIO_45 {
				line_name = "LED_ETH_YELLOW_2";
			};
			GPIO_7 {
				line_name = "LED_TECH_GEN_RED";
				active_low;
			};
			GPIO_12 {
				line_name = "LED_TECH_GEN_GREEN";
				active_low;
			};
			GPIO_8 {
				line_name = "LED_TECH_GEN_BLUE";
				active_low;
			};
			GPIO_47 {
				line_name = "LED_SSID_RED";
				active_low;
			};
			GPIO_46 {
				line_name = "LED_SSID_GREEN";
				active_low;
			};
			GPIO_48 {
				line_name = "LED_SSID_BLUE";
				active_low;
			};
		};

		poeman {
			GPIO_16 {
				line_name = "GPIO_4PIN_SIGNAL";
				//active_low
			};
			GPIO_18 {
				line_name = "GPIO_T2P_SIGNAL";
			};
			GPIO_6 {
				line_name = "GPIO_PG_SIGNAL";
			};
			GPIO_44 {
				line_name = "GPIO_PSE_EN";
			};
		};

		misc {
			GPIO_13 {
				line_name = "GPIO_MODEM_RESET_1";
			};
			GPIO_17 {
				line_name = "GPIO_MODEM_POWER_1";
			};
		};
	};

	poeman {
		bus = "/dev/i2c-0";
		pse_en = "GPIO_PSE_EN";
		budget@0 {
			GPIO_PG_SIGNAL = <0>;
			budget = <0>;
		};
		budget@1 {
			GPIO_T2P_SIGNAL = <0>;
			GPIO_PG_SIGNAL = <1>;
			budget = <30000>;
		};
		budget@2 {
			GPIO_T2P_SIGNAL = <1>;
			GPIO_PG_SIGNAL = <1>;
			budget = <15000>;
		};
	};
};

&state_default {
	gpio {
		groups = "wdt", "jtag", "sdhci", "uart2", "uart3";
		function = "gpio";
	};
};

&switch0 {
	ports {
		port@1 {
			status = "okay";
			label = "lan";
		};
	};
};

&mnfinfo {
	sim_cfg {
		default = "1111020_1112020";
		keep-sim-presence;
	};
};

&i2c {
	status = "okay";

	hwmon@0x48 {
		compatible = "ti,tla2021";
		reg = <0x48>;
	};

	tpm: tpm@2e {
		compatible = "infineon,slb9673";
		reg = <0x2e>;
		status = "okay";
	};
};

&mdio {
	ephy4: ephy@4 {
		reg = <0>;
	};
};
