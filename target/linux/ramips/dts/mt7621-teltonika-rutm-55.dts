#include "mt7621-teltonika-rutm-common.dtsi"

/ {
	compatible = "teltonika,rutm", "mediatek,mt7621-soc";
	model = "Teltonika RUTM55";

	ext_io {
		compatible = "spi-gpio";
		#address-cells = <1>;
		#size-cells = <0>;

		gpio-sck = <&gpio 45 GPIO_ACTIVE_HIGH>;	// 74HC595 SRCLK (Serial Clock)
		gpio-mosi = <&gpio 13 GPIO_ACTIVE_HIGH>;	// 74HC595 SER (Serial)
		cs-gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;	// 74HC595 RCLK (Register Clock)
		num-chipselects = <1>;

		shift_io: shift_io@0 {
			compatible = "fairchild,74hc595";
			reg = <0>;
			gpio-controller;
			#gpio-cells = <2>;
			registers-number = <2>;
			spi-max-frequency = <10000000>;
			enable-gpios = <&gpio 44 GPIO_ACTIVE_LOW>;

		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 10 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		gpio_modem_reset {
			gpio-export,name = "modem_reset";
			gpio-export,output = <0>;
			gpios = <&gpio 14 GPIO_ACTIVE_HIGH>;
		};

		gpio_modem_power {
			gpio-export,name = "modem_power";
			gpio-export,output = <1>; // tmp
			gpios = <&gpio 17 GPIO_ACTIVE_HIGH>;
		};

		gpio_sim_select {
			gpio-export,name = "sim_sel";
			gpio-export,output = <1>;
			gpios = <&gpio 46 GPIO_ACTIVE_HIGH>;
		};

		gpio_dcdc_enable{
			gpio-export,name = "dcdc_enable";
			gpio-export,output = <0>;
			gpios = <&gpio 9 GPIO_ACTIVE_HIGH>;
		};

		/*--------------------*/
		/*-  FOR TEST BATCH  -*/
		/*--------------------*/
		/*yellow_eth_port_3 {*/
			/*gpio-export,name = "yellow:eth_port_3";*/
			/*gpio-export,output = <1>;*/
			/*gpios = <&shift_io 1 GPIO_ACTIVE_LOW>;*/
		/*};*/

		/*yellow_eth_port_2 {*/
			/*gpio-export,name = "yellow:eth_port_2";*/
			/*gpio-export,output = <0>;*/
			/*gpios = <&shift_io 3 GPIO_ACTIVE_HIGH>;*/
