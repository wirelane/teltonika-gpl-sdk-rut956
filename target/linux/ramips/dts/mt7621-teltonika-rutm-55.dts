#include "common-mt7621/rutm.dtsi"
#include "common-mt7621/shiftreg.dtsi"
#include "common-mt7621/tpm-tis.dtsi"
#include "common-mt7621/modem/5g.dtsi"

// PWR, RST       |G0|G10|
// IN, OUT, ISOL  |G12|G16|G18|S15|
// SR CTRL        |G9|G13|G44|G45|
// ETH            |S0-S7|
// 5G		      |G14|G17|G46|S9-G14|
// MISC           |S8|
/ {
	model = "Teltonika RUTM55";

	ext_io {
		gpio-mosi = <&gpio 13 GPIO_ACTIVE_HIGH>;
		cs-gpios = <&gpio 45 GPIO_ACTIVE_HIGH>;
	};

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		gpio_sim_select {
			gpio-export,name = "sim_sel";
			gpio-export,output = <1>;
			gpios = <&gpio 46 GPIO_ACTIVE_HIGH>;
		};
	};

	leds {
		led_gen_3 {
			gpios = <&shift_io 9 GPIO_ACTIVE_HIGH>;
		};

		led_gen_4 {
			gpios = <&shift_io 10 GPIO_ACTIVE_HIGH>;
		};

		led_gen_5 {
			gpios = <&shift_io 11 GPIO_ACTIVE_HIGH>;
		};

		led_ssid_1 {
			gpios = <&shift_io 12 GPIO_ACTIVE_HIGH>;
		};

		led_ssid_3 {
			gpios = <&shift_io 13 GPIO_ACTIVE_HIGH>;
		};

		led_ssid_5 {
			gpios = <&shift_io 14 GPIO_ACTIVE_HIGH>;
		};

		led_eth_y3 {
			label = "eth_y3";
			gpios = <&shift_io 1 GPIO_ACTIVE_HIGH>;
		};

		led_eth_y2 {
			label = "eth_y2";
			gpios = <&shift_io 3 GPIO_ACTIVE_HIGH>;
		};

		led_eth_y1 {
			label = "eth_y1";
			gpios = <&shift_io 5 GPIO_ACTIVE_HIGH>;
		};

		led_eth_y0 {
			label = "eth_y0";
			gpios = <&shift_io 7 GPIO_ACTIVE_HIGH>;
		};

		led_eth_g3 {
			label = "eth_g3";
			gpios = <&shift_io 0 GPIO_ACTIVE_HIGH>;
		};

		led_eth_g2 {
			label = "eth_g2";
			gpios = <&shift_io 2 GPIO_ACTIVE_HIGH>;
		};

		led_eth_g1 {
			label = "eth_g1";
			gpios = <&shift_io 4 GPIO_ACTIVE_HIGH>;
		};

		led_eth_g0 {
			label = "eth_g0";
			gpios = <&shift_io 6 GPIO_ACTIVE_HIGH>;
		};
	};

	tlt_gpios {
		ioman {
			GPIO_16 {
				line_name = "GPIO_IN_1";
				active_low;
			};
			GPIO_111 {
				line_name = "GPIO_OUT_1";
			};
			GPIO_18 {
				line_name = "GPIO_ISOLATED_INPUT";
				active_low;
			};
			GPIO_12 {
				line_name = "GPIO_RELAY_CLOSE_1";
			};
		};

		misc {
			GPIO_46 {
				line_name = "GPIO_SIM_SELECT";
			};
			GPIO_104 {
				line_name = "GPIO_RS485_RX_EN";
			};
		};
	};

	ioman {
		layout@0 {
			io@0 { // 16pin#1
				label = "din1";
				block_type = BLOCK_16PIN;
				block_pin = "1";
				web_str = "Digital Input";
				gpio_in = "GPIO_IN_1";
				io_param = "g2";
			};
			io@1 { // 16pin#2
				label = "iio";
				block_type = BLOCK_16PIN;
				block_pin = "2,10";
				gpio_in = "GPIO_ISOLATED_INPUT";
				io_param = "g3";
			};
			io@2 { // 16pin#3
				label = "dout1";
				block_type = BLOCK_16PIN;
				block_pin = "3,4,11";
				web_str = "Isolated Output";
				gpio_out = "GPIO_OUT_1";
				io_param = "g4";
			};
			relay@0 {
				label = "relay0";
				block_type = BLOCK_16PIN;
				block_pin = "5,13";
				gpio_close = "GPIO_RELAY_CLOSE_1";
				io_param = "g5";
			};
			adc@0 {
				label = "adc0";
				block_type = BLOCK_16PIN;
				block_pin = "12";
				tolerance = "0.05";
				path = "/sys/class/hwmon/hwmon0/in0_input";
				io_param = "g1";

				adc_cof@0 {
					divider = "128.160418482999"; //147.0 / 1147.0 * 1000.0
				};
			};
			acl@0 {
				label = "acl0";
				block_type = BLOCK_16PIN;
				block_pin = "12";
				tolerance = "0.00004";
				path = "/sys/class/hwmon/hwmon0/in0_input";
				io_param = "g0";

				adc_cof@0 {
					divider = "153792.502179599"; //147.0 / 1147.0 * 1000.0 * 1200.0
				};
			};
		};
	};
};

&state_default {
	gpio {
		groups = "wdt", "jtag", "sdhci", "uart2";
	};
};

&switch0 {
	ports {
		port@0 {
			status = "okay";
			label = "lan1";
		};

		port@1 {
			status = "okay";
			label = "lan2";
		};

		port@2 {
			status = "okay";
			label = "lan3";
		};
	};
};

&mnfinfo {
	sim_cfg {
		default = "1111200_1112000_1121100";
		/*default = "1111020_1112020";*/
	};
};

&uartlite3 {
	status = "okay";
};

&i2c {
	hwmon@0x48 {
		compatible = "ti,tla2021";
		reg = <0x48>;
	};
};
