		/*yellow_eth_port_0 {*/
			/*gpio-export,name = "yellow:eth_port_0";*/
			/*gpio-export,output = <0>;*/
			/*gpios = <&shift_io 7 GPIO_ACTIVE_HIGH>;*/
		/*};*/

		/*green_eth_port_3 {*/
			/*gpio-export,name = "green:eth_port_3";*/
			/*gpio-export,output = <1>;*/
			/*gpios = <&shift_io 0 GPIO_ACTIVE_LOW>;*/
		/*};*/

		/*green_eth_port_2 {*/
			/*gpio-export,name = "green:eth_port_2";*/
			/*gpio-export,output = <0>;*/
			/*gpios = <&shift_io 2 GPIO_ACTIVE_HIGH>;*/
		/*};*/

		/*green_eth_port_1 {*/
			/*gpio-export,name = "green:eth_port_1";*/
			/*gpio-export,output = <0>;*/
			/*gpios = <&shift_io 4 GPIO_ACTIVE_HIGH>;*/
		/*};*/

		/*green_eth_port_0 {*/
			/*gpio-export,name = "green:eth_port_0";*/
			/*gpio-export,output = <0>;*/
			/*gpios = <&shift_io 6 GPIO_ACTIVE_HIGH>;*/
		/*};*/
		/*------------------------*/
		/*-  FOR TEST BATCH END  -*/
		/*------------------------*/

		/*--------------------*/
		/*-  FOR PROTOTYPES  -*/
		/*--------------------*/
		yellow_eth_port_2 {
			gpio-export,name = "yellow:eth_port_2";
			gpio-export,output = <1>;
			gpios = <&shift_io 1 GPIO_ACTIVE_LOW>;
		};

		yellow_eth_port_1 {
			gpio-export,name = "yellow:eth_port_1";
			gpio-export,output = <0>;
			gpios = <&shift_io 3 GPIO_ACTIVE_HIGH>;
		};

		yellow_eth_port_0 {
			gpio-export,name = "yellow:eth_port_0";
			gpio-export,output = <0>;
			gpios = <&shift_io 5 GPIO_ACTIVE_HIGH>;
		};

		yellow_eth_port_3 {
			gpio-export,name = "yellow:eth_port_3";
			gpio-export,output = <0>;
			gpios = <&shift_io 7 GPIO_ACTIVE_HIGH>;
		};

		green_eth_port_2 {
			gpio-export,name = "green:eth_port_2";
			gpio-export,output = <1>;
			gpios = <&shift_io 0 GPIO_ACTIVE_LOW>;
		};

		green_eth_port_1 {
			gpio-export,name = "green:eth_port_1";
			gpio-export,output = <0>;
			gpios = <&shift_io 2 GPIO_ACTIVE_HIGH>;
		};

		green_eth_port_0 {
			gpio-export,name = "green:eth_port_0";
			gpio-export,output = <0>;
			gpios = <&shift_io 4 GPIO_ACTIVE_HIGH>;
		};

		green_eth_port_3 {
			gpio-export,name = "green:eth_port_3";
			gpio-export,output = <0>;
			gpios = <&shift_io 6 GPIO_ACTIVE_HIGH>;
		};
