		/*-  FOR PROTOTYPES END  -*/
		/*------------------------*/
	};

	leds {
		compatible = "gpio-leds";

		led_power {
			label = "power";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
			default-state = "on";
			retain-state-shutdown;
		};

		led_gen_3 {
			label = "mob_gen_3";
			gpios = <&shift_io 9 GPIO_ACTIVE_HIGH>;
		};

		led_gen_4 {
			label = "mob_gen_4";
			gpios = <&shift_io 10 GPIO_ACTIVE_HIGH>;
		};

		led_gen_5 {
			label = "mob_gen_5";
			gpios = <&shift_io 11 GPIO_ACTIVE_HIGH>;
		};

		led_ssid_1 {
			label = "mob_ssid_1";
			gpios = <&shift_io 12 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "timer"; // only
		};

		led_ssid_3 {
			label = "mob_ssid_3";
			gpios = <&shift_io 13 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "timer"; // only
		};

		led_ssid_5 {
			label = "mob_ssid_5";
			gpios = <&shift_io 14 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "timer"; //only
		};
	};

	tlt_gpios { // Modify according to wiki page "DTS syntax with Libgpiod changes"
		gpiochip_info {
			hw_0 {
				hwver = <0>;
				gpiochip_count = <4>;

				gpiochip_0 = <32>; // Built-in
				gpiochip_1 = <32>; // Built-in
				gpiochip_2 = <32>; // Built-in
				gpiochip_3 = <16>; // Shift Register
			};
		};

		ioman {
			GPIO_16 {
				line_name = "GPIO_IN_1";
				active_low;
			};
			GPIO_111 {
				line_name = "GPIO_OUT_1";
			};
			GPIO_18 {
				line_name = "GPIO_ISOLATED_INPUT";
				active_low;
			};
			GPIO_12 {
				line_name = "GPIO_RELAY_CLOSE_1";
			};
		};

		ledman {
			GPIO_0 {
				line_name = "LED_POWER";
				active_low;
			};
			/*--------------------*/
			/*-  FOR PROTOTYPES  -*/
			/*--------------------*/
			GPIO_96 {
				line_name = "LED_ETH_GREEN_3";
				active_low;
			};
			GPIO_97 {
				line_name = "LED_ETH_YELLOW_3";
			};
			GPIO_98 {
				line_name = "LED_ETH_GREEN_2";
			};
			GPIO_99 {
				line_name = "LED_ETH_YELLOW_2";
			};
			GPIO_100 {
				line_name = "LED_ETH_GREEN_1";
			};
			GPIO_101 {
				line_name = "LED_ETH_YELLOW_1";
			};
			GPIO_102 {
				line_name = "LED_ETH_GREEN_4";
			};
			GPIO_103 {
				line_name = "LED_ETH_YELLOW_4";
			};
			/*----------------------*/
			/*-  FOR PROTOTYPES END-*/
			/*----------------------*/

			/*--------------------*/
			/*-  FOR TEST BATCH  -*/
			/*--------------------*/
			/*GPIO_96 {*/
				/*line_name = "LED_ETH_GREEN_4";*/
				/*active_low;*/
			/*};*/
			/*GPIO_97 {*/
				/*line_name = "LED_ETH_YELLOW_4";*/
			/*};*/
			/*GPIO_98 {*/
				/*line_name = "LED_ETH_GREEN_3";*/
			/*};*/
			/*GPIO_99 {*/
				/*line_name = "LED_ETH_YELLOW_3";*/
			/*};*/
			/*GPIO_100 {*/
				/*line_name = "LED_ETH_GREEN_2";*/
			/*};*/
			/*GPIO_101 {*/
				/*line_name = "LED_ETH_YELLOW_2";*/
			/*};*/
			/*GPIO_102 {*/
				/*line_name = "LED_ETH_GREEN_1";*/
			/*};*/
			/*GPIO_103 {*/
				/*line_name = "LED_ETH_YELLOW_1";*/
			/*};*/
			/*------------------------*/
			/*-  FOR TEST BATCH END  -*/
			/*------------------------*/
			GPIO_105 {
				line_name = "LED_TECH_GEN_3";
			};
			GPIO_106 {
				line_name = "LED_TECH_GEN_4";
			};
			GPIO_107 {
				line_name = "LED_TECH_GEN_5";
			};
			GPIO_108 {
				line_name = "LED_SSID_1";
			};
			GPIO_109 {
				line_name = "LED_SSID_3";
			};
			GPIO_110 {
				line_name = "LED_SSID_5";
			};
		};

		misc {
			GPIO_9 {
				line_name = "GPIO_DCDC_ENABLE";
			};
			GPIO_14 {
				line_name = "GPIO_MODEM_RESET_1";
			};
			GPIO_17 {
				line_name = "GPIO_MODEM_POWER_1";
			};
			GPIO_46 {
				line_name = "GPIO_SIM_SELECT";
			};
			GPIO_104 {
				line_name = "GPIO_RS485_RX_EN";
			};
		};
	};

	ioman {
		layout@0 {
			io@0 { // 16pin#1
				label = "din1";
				block_type = BLOCK_16PIN;
				block_pin = "1";
				web_str = "Digital Input";
				gpio_in = "GPIO_IN_1";
				io_param = "g2";
			};
			io@1 { // 16pin#2
				label = "iio";
				block_type = BLOCK_16PIN;
				block_pin = "2,10";
				gpio_in = "GPIO_ISOLATED_INPUT";
				io_param = "g3";
			};
			io@2 { // 16pin#3
				label = "dout1";
				block_type = BLOCK_16PIN;
				block_pin = "3,4,11";
				web_str = "Isolated Output";
				gpio_out = "GPIO_OUT_1";
				io_param = "g4";
			};
			relay@0 {
				label = "relay0";
				block_type = BLOCK_16PIN;
				block_pin = "5,13";
				gpio_close = "GPIO_RELAY_CLOSE_1";
				io_param = "g5";
			};
			adc@0 {
				label = "adc0";
				block_type = BLOCK_16PIN;
				block_pin = "6,9";
				tolerance = "0.05";
				path = "/sys/class/hwmon/hwmon0/device/in0_input";
				io_param = "g1";

				adc_cof@0 {
					divider = "128.160418482999"; //147.0 / 1147.0 * 1000.0
				};
			};
			acl@0 {
				label = "acl0";
				block_type = BLOCK_16PIN;
				block_pin = "6,9";
				tolerance = "0.00004";
				path = "/sys/class/hwmon/hwmon0/device/in0_input";
				io_param = "g0";

				adc_cof@0 {
					divider = "153792.502179599"; //147.0 / 1147.0 * 1000.0 * 1200.0
				};
			};
		};
	};
};

&state_default {
	gpio {
		groups = "wdt", "jtag", "sdhci", "uart2";
		function = "gpio";
	};
};

&switch0 {
	ports {
		port@1 {
			status = "okay";
			label = "lan1";
		};

		port@2 {
			status = "okay";
			label = "lan2";
		};

		port@3 {
			status = "okay";
			label = "lan3";
		};
	};
};

&mnfinfo {
	sim_cfg {
		default = "1111020_1112020";
	};
};

&uartlite3 {
	status = "okay";
};

&i2c {
	status = "okay";

	hwmon@0x48 {
		compatible = "ti,tla2021";
		reg = <0x48>;
	};

	tpm: tpm@2e {
		compatible = "infineon,slb9673";
		reg = <0x2e>;
		status = "okay";
	};
};

/*--------------------*/
/*-  FOR PROTOTYPES  -*/
/*--------------------*/

&mdio {
	ephy4: ephy@4 {
		reg = <0>;
	};
};
/*----------------------*/
/*-  FOR PROTOTYPES END-*/
/*----------------------*/
